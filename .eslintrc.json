{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jquery": true,
        "node": true
    },
    "extends": [      
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "space-before-function-paren": ["error", "never"],
        "indent": "off",
        "linebreak-style": "off"
    }
}
